# My custom aliases | $ source ~/.bashrc
alias a='alias'
alias grep='grep --color=auto'
alias ll='ls -lha --color=auto'
alias ..='cd ..'
alias ...='cd ../..'
alias c='clear'
alias g='git status'
alias gl='git log --oneline --decorate --graph'
alias gp="git add . && git commit -m 'wip' && git push"
alias h='history'
alias p='ping'
alias ps='ps aux'
alias du='du -sh'
alias df='df -h'
alias o='xdg-open'
alias remote='cd && ./MuggleBornPadawan/700_linux/remote_startup.sh'
alias commit='cd && ./MuggleBornPadawan/700_linux/bckp/commits.sh'
alias start='cd && ./MuggleBornPadawan/700_linux/bckp/start.sh'
alias pw='pass git/github.com/MuggleBornPadawan'
# Safety & Security Aliases
alias cp='cp -i'
alias mv='mv -i'
alias ln='ln -i'
alias rm='rm -i'
alias rm="trash-put"
# File Management Aliases
alias less='less -r'
alias get='curl -O'
alias mkdir='mkdir -p'