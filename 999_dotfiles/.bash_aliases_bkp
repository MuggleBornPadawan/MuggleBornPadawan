# My custom aliases | $ source ~/.bashrc 
alias s='source ~/.bashrc'
alias a='alias'
alias t='tmux a'
alias tn='tmux new -s "alpha"'
alias td='tmux detach'
alias tk='tmux kill-server'
alias e='emacs -nw'
alias j='r && history -w && history -c && rm .bash_history && c && echo "clean slate protocol activated"'
alias grep='grep --color=auto'
alias l='ls -lha --color=auto'
alias r='cd'
alias ..='cd ..'
alias ...='cd ../..'
alias m='r && cd MuggleBornPadawan && l'
alias w1='r && cd MuggleBornPadawan/100_cpp/randomwalker/ && l'
alias c='clear'
alias g='git status'
alias gl='git log --oneline --decorate --graph'
alias gp="touch test.tmp && echo 'meowww at $(date)' >> test.tmp && git add . && git commit -m 'wip $(date +%s)' && git push"
alias h='history'
alias p='ping'
alias ps='ps aux'
alias du='du -sh'
alias df='df -h'
alias o='xdg-open'
alias remote='cd && ./MuggleBornPadawan/700_linux/remote_startup.sh'
alias commit='cd && ./MuggleBornPadawan/700_linux/bckp/commits.sh'
alias start='cd && ./MuggleBornPadawan/700_linux/bckp/start.sh'
alias test_wisdom='cd && ./MuggleBornPadawan/700_linux/scripts/wisdom_nugget_generator.sh'
alias test_math='cd && ./MuggleBornPadawan/700_linux/scripts/gemini_artithmetic_test.sh'
alias test_aeo='cd && ./MuggleBornPadawan/700_linux/scripts/openrouter_aeo_script.sh'
alias pw='pass unKnoWn'
# Safety & Security Aliases
alias ln='ln -i'
alias rm='rm -i'
alias rm="trash-put"
# File Management Aliases
alias less='less -r'
alias get='curl -O'
alias mkdir='mkdir -p'
# npm
alias nu='npm list -g --depth=0 && npm outdated && npm update &&  npm audit fix --force && npm install -g npm-check-updates'
alias abc='echo "test"'